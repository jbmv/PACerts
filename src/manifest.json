{
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {"js": ["contentScriptMJ.js"], "matches": ["https://*.mjplatform.com/*"], "run_at": "document_start"},
    {"js": ["contentScriptDOH.js"], "matches": ["https://*.padohmmp.custhelp.com/*"], "run_at": "document_start"}
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "description": "Maintains a list of patients and their certification status for PA MMJ pharmacists",
  "externally_connectable": {
    "matches": [ "*://app.mjplatform.com/*" ]
  },
  "host_permissions": [ "https://*.mjplatform.com/*", "https://*.padohmmp.custhelp.com/*" ],
  "incognito": "split",
  "key": "",
  "manifest_version": 3,
  "name": "PACerts",
  "options_page": "options.html",
  "permissions": [ "activeTab", "storage", "unlimitedStorage", "tabs", "webRequest", "clipboardWrite" ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1",
  "web_accessible_resources": [ {
    "matches": [ "*://app.mjplatform.com/*", "*://padohmmp.custhelp.com/*" ],
    "resources": [ "injectMJ.js", "injectDOH.js", "DOHbanner.html" ]
  } ]
}